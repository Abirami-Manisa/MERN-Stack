{
  "name": "employee-management-system-app",
  "version": "0.1.0",
  "author": "Abirami Manisa Sakthivel",
  "description": "Employee Management System",
  "main": "public/electron.js",
  "private": true,
  "homepage": "./",
  "type": "commonjs",
  "proxy": "http://localhost:5001",
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.25.0",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "jwt-decode": "^3.1.2",
    "material-ui-chip-input": "^1.1.0",
    "materialui-pagination": "^0.0.6",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-dom": "^17.0.2",
    "react-file-base64": "^1.0.3",
    "react-google-login": "^5.2.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "3.4.1",
    "reactjs-popup": "^2.0.5",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "wait-on": "^6.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-package": "electron-builder -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "productName": "Employee Management System",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "build/ems.png"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0"
  }
}
